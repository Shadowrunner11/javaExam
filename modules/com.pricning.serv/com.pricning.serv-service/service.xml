<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_2_0.dtd">

<service-builder dependency-injector="ds" package-path="com.pricning.serv">
	<namespace>PRINCING</namespace>
	<!--<entity data-source="sampleDataSource" local-service="true" name="Foo" remote-service="false" session-factory="sampleSessionFactory" table="foo" tx-manager="sampleTransactionManager uuid="true"">-->
	<entity local-service="true" name="Pricing" remote-service="true" uuid="true">

		<!-- PK fields -->

		<column name="id" primary="true" type="long" />


		<column name="firstName" type="String"/>
		<column name="lastName" type="String" />
		<column name="carModel" type="String" />
		<column name="identityDocument" type="String" />
		<column name="sucursal" type="String" />
		<column name="department" type="String" />
		<column name="province" type="String" />
		<column name="productType" type="String" />
		<column name="productModel" type="String" />
		<column name="productVersion" type="String" />
		<column name="contactChannel" type="String" />
		<column name="phone" type="String" />
		<column name="clientType" type="String" />
		<column name="email" type="String" />
		

		<reference entity="AssetEntry" package-path="com.liferay.portlet.asset" />
		<reference entity="AssetTag" package-path="com.liferay.portlet.asset" />
	</entity>
	<entity local-service="true" name="Modelos" remote-service="true" uuid="true">
		<column name="id" primary="true" type="long" />

		<column name="modeloName" type="String" />
	</entity>
</service-builder>
